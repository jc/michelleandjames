{
    "version": 1,
    "name": "michelleandjames",
    "type": "static",
    "alias": ["michelleandjames.net"],
    "static": {
        "directoryListing": false,
        "rewrites": [
            { "source": "/", "destination": "/jackson.html" }
        ],
        "headers": [
        {
            "source": "**/*.@(jpg|jpeg|gif|png)",
            "headers": [{
                "key": "Cache-Control",
                "value": "public, max-age=2628000"
            }]
        }
        ]
    }
}
